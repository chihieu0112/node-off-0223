document.addEventListener('DOMContentLoaded', function () {
  jQuery(document).ready(function ($) {
    $(document).on('click', '[rel-script="nav-tab"]', function (e) {
      e.preventDefault();
      let _this = $(this);
      let id = _this.attr('href');
      let tab = $('[data-nav-tab="' + id + '"]');
      _this.addClass('is-current');
      _this.siblings().removeClass('is-current');
      tab.show();
      tab.siblings('[data-nav-tab]').hide();
    });
    $(document).on('click', '[rel-script="mobile-menu-toggle"]>a', function (e) {
      e.preventDefault();
      const _this = $(this);
      _this.siblings('.nav-child-menu').slideToggle();
      _this.toggleClass('toggle-menu');
    })

    let x = 0, x_end = 0;
    let y = 0, y_end = 0;
    let time = 300;
    let active = false;

    $(document).on('touchstart', '.nav-tab__content', function (e) {
      x = e.originalEvent.touches[0].pageX;
      y = e.originalEvent.touches[0].pageY;
    });
    $(document).on('touchend', '.nav-tab__content', function (e) {
      if (active) return;
      x_end = e.changedTouches[0].pageX;
      y_end = e.changedTouches[0].pageY;

      if (y - y_end > 100) return;
      if (x - x_end < 50 && x - x_end > -50) return;

      const tab = $('.is-current[rel-script="nav-tab"]');
      const tab_header = tab.parent();
      if (x > x_end) {
        tab.addClass('left-animation');
        if (tab.next().length) {
          tab.next().click();
          tab.next().addClass('left');
        }
      }
      if (x < x_end) {
        tab.addClass('right-animation');
        if (tab.prev().length) {
          tab.prev().click();
          tab.prev().addClass('right');
        }
      }
      active = true;
      setTimeout(function (e) {
        tab.removeClass('right-animation left-animation')
        tab.prev().removeClass('right');
        tab.next().removeClass('left');
        active = false;
      }, time)
    });

  });
});
</script>
<script type="text/javascript">
document.addEventListener('DOMContentLoaded', function () {
  const spotlight_search_control = '[rel-script="spotlight-search-control"]',
    header_search_content = '[rel-script="header-search-content"]',
    spotlight_search = '[rel-script="spotlight-search"]',
    spotlight_search_close = '[rel-script="spotlight-search-close"]';

  const header = $('header'),
    header_search = $(header_search_content),
    search_input = $(spotlight_search_control),
    spotlight_wrapper = $(spotlight_search);

  const loading = spotlight_wrapper.html();


  const mega_menu = '[rel-script="mega-menu"]',
    mega_menu_item = '[rel-script="mega-menu-item"]',
    mega_menu_toggle = '[rel-script="mega-menu-toggle"]',
    mega_menu_active = '[rel-script="mega-menu-active"]',
    mega_menu_close = '[rel-script="mega-menu-close"]',
    mega_menu_open = '[rel-script="mega-menu-open"]',
    header_menu = '[rel-script="header-menu"]',
    menu_toggle = '[rel-script="menu-toggle"]';
  search_toggle = '[rel-script="search-toggle"]';

  window.dataLayer = window.dataLayer || [];

  spotlight_wrapper.on('click', 'a', function () {
    const productClickedArray = [
      {
        'name': $(this).find('h4').text().trim(),
        'price': parseInt($(this).find('ins').text().replace(/[.đ]+/g, '')),
        'category': '',
        'list': 'Search: ' + search_input.val() + ' - Recommendation',
        'position': spotlight_wrapper.find('a').index(this) + 1
      }
    ];
    const ecommerceObject = {
      'ecommerce': {
        'click': {
          'products': productClickedArray,
          'actionField': { 'list': 'Search: ' + search_input.val() + ' - Recommendation' }
        }
      }, 'event': 'productClick'
    }
    dataLayer.push(ecommerceObject)
  })

  function handleSearch(keyword) {
    const API_URL = `${window.location.origin}/load-product?home_search=1&limit=5&page=1&sort=created_at&type=spotlight&keyword=${keyword}`;
    let search_resource = '<div class="text--center"><i>Không tìm thấy kết quả phù hợp!</i></div>'

    spotlight_wrapper.parent().addClass('is-active')
    spotlight_wrapper.html(loading);

    if ($(spotlight_search_close).length > 0) {
      $(spotlight_search_close).addClass('is-active');
    }

    $.ajax({
      url: API_URL,
      type: "GET",
      cache: false,
    }).done(function (res) {
      if (res.success) {
        search_resource = res.html ? res.html : search_resource;
        spotlight_wrapper.html(search_resource);
        spotlight_wrapper.addClass('is-active');
        if (res.html != '') {
          spotlight_wrapper.siblings('.view-more').show();
          spotlight_wrapper.siblings('.view-more').attr('href', '/spotlight?keyword=' + keyword)
        } else {
          spotlight_wrapper.siblings('.view-more').hide();
        }

        if (res.html.length === 0) {
          dataLayer.push({
            'event': 'searchNoResults',
            'searchTerm': keyword
          });
        }
        else {
          const productImpressionArray = [];

          spotlight_wrapper.find('a').each((index, item) => {
            productImpressionArray.push({
              'name': $(item).find('h4').text().trim(),
              'price': parseInt($(item).find('ins').text().replace(/[.đ]+/g, '')),
              'brand': 'Coolmate',
              'category': '',
              'list': 'Search: ' + keyword + ' - Recommendation',
              'position': index + 1
            })
          })
          const ecommerceObject = {
            'ecommerce': { 'impressions': productImpressionArray },
            'event': 'productImpressions'
          }
          dataLayer.push(ecommerceObject);
          window.dataLayer.push({
            'dr_event_type': 'view_search_results',
            'dr_items': productImpressionArray.map(item => ({
              'id': item.id,
              'google_business_vertical': 'retail'
            })),
            'event': 'dynamic_remarketing'
          });
        }
      }
    }).fail(() => {
      spotlight_wrapper.html(search_resource);
      dataLayer.push({
        'event': 'searchNoResults',
        'searchTerm': keyword
      });
    });
  }

  function removeActiveHeader() {
    search_input.val('');
    header_search.removeClass('is-active');
    spotlight_wrapper.parent().removeClass('is-active');
  }

  function headerSearch() {

    let targetSearch;
    let last_keyword;
    let search_time_out;

    $(document).on('click', '[rel-script="header-search"]', function (e) {
      e.preventDefault();
      header_search.addClass('is-active');
      targetSearch = e.target;
      $('.nav__sub').siblings().removeClass('is-active')
      setTimeout(function (e) {
        search_input.focus();
      }, 100)
    });

    $(document).on('click', function (e) {
      // const _target = $(e.target);
      // if (!targetSearch) return;
      // if (!_target.parents('[rel-script="header-search"]').hasClass('is-active') && e.target != targetSearch) {
      //     header_search.removeClass('is-active');
      // }
    });

    search_input.on('blur', function (e) {
      header_search.removeClass('is-active');
    })

    $(document).on('click', spotlight_search_close, function (e) {
      e.preventDefault();
      removeActiveHeader();
      $(this).removeClass('is-active');
    })

    search_input.on('keyup', function (e) {
      const _this = $(this);
      const value = _this.val();

      if (!value) return;
      if (value === last_keyword) return;
      if (search_time_out !== undefined) clearTimeout(search_time_out);

      last_keyword = value;
      search_time_out = setTimeout(function (e) {
        handleSearch(value);
      }, 500)
    })
  }

  function headerScroll() {
    let last_scroll_point = $(window).scrollTop();


    if (last_scroll_point > 0) {
      header.addClass('is-scroll-top');
    }

    $(window).on('scroll', function (e) {
      const scroll_point = $(this).scrollTop();
      if (!$(header_menu).hasClass('is-active')) {
        if (scroll_point > last_scroll_point && scroll_point > 100) {
          header.addClass('is-scroll-top');
          header_search.removeClass('is-active');
          spotlight_wrapper.parent().removeClass('is-active');
          search_input.val('');
        } else {
          if (header.hasClass('is-scroll-top')) {
            header.removeClass('is-scroll-top');
          }
        }
      }
      last_scroll_point = scroll_point;
    });
  }

  function removeActiveMenuMobile() {
    $(header_menu).removeClass('is-active');
    $(menu_toggle).removeClass('is-active');
    $('body').removeClass('backdrop');
    removeActiveHeader();
  }
  function checkResize() {
    return $(window).width() < 1025;
  }

  function subMenuToggle() {
    $('[rel-script="sub-menu"]').mouseover(function (e) {
      e.preventDefault();
      const _this = $(this);
      const menuId = _this.attr('data-menu-id');
      const nav_sub = $('.nav__sub');
      const nav_sub_curent = $('.nav__sub#' + menuId);
      nav_sub.removeClass('hide-item');
      const nav_sub_siblings = nav_sub.siblings();
      if (nav_sub_curent.length > 0) {
        nav_sub_siblings.removeClass('is-active');
        nav_sub_siblings.addClass('hide-item');
        nav_sub_curent.addClass('is-active');
        nav_sub_curent.removeClass('hide-item');
      } else {
        nav_sub.removeClass('is-active');
      }
    });

    $('main').mouseover(function (e) {
      e.preventDefault();
      const nav_sub = $('.nav__sub');
      nav_sub.removeClass('is-active');
      nav_sub.removeClass('hide-item');
    })

    $('.h4d').mouseover(function (e) {
      e.preventDefault();
      const nav_sub = $('.nav__sub');
      nav_sub.removeClass('is-active');
      nav_sub.removeClass('hide-item');
    })

    $('.site-content').mouseover(function (e) {
      e.preventDefault();
      const nav_sub = $('.nav__sub');
      nav_sub.removeClass('is-active');
      nav_sub.removeClass('hide-item');
    })
  }

  function megaMenuToggle() {
    $(document).on('click', mega_menu_toggle, function (e) {
      e.preventDefault();
      if (checkResize()) {
        const _this = $(this);
        const menu_item_siblings = _this.closest(mega_menu_item).siblings();
        if (!_this.hasClass('is-active')) {
          menu_item_siblings.find(mega_menu_active).slideUp();
          menu_item_siblings.find(mega_menu_toggle).removeClass('is-active')
        }
        _this.siblings(mega_menu_active).slideToggle();
        _this.toggleClass('is-active');
      }
    });

    $(document).on('click', mega_menu_open, function (e) {
      if (checkResize()) {
        e.preventDefault();
        const _this = $(this);
        const menu = _this.parents(mega_menu);
        menu.addClass('is-active');
        menu.siblings().removeClass('is-active');
      }
    });

    $(document).on('click', mega_menu_close, function (e) {
      e.preventDefault();
      if (checkResize()) {
        const _this = $(this);
        _this.parents(mega_menu).removeClass('is-active');
      }
    });

    $(document).on('click', menu_toggle, function (e) {
      e.preventDefault(e);
      if (checkResize()) {
        const _this = $(this);
        const _menu = $(header_menu);
        const _menuProduct = $('[rel-script="mega-menu-product"]');
        _menu.toggleClass('is-active');
        _menu.find('.tablet--visible').show();
        _menu.find('.tablet--hidden').hide();
        _this.toggleClass('is-active');
        $('body').toggleClass('backdrop');
        _menuProduct.find('[rel-script="mega-menu-toggle"]').addClass('is-active');
        _menuProduct.find('[rel-script="mega-menu-active"]').addClass('is-active');
        _menuProduct.find('[rel-script="mega-menu-active"]').show();
        removeActiveHeader();
      }
    });

    $(document).on('click', function (e) {
      if ($(e.target).hasClass('backdrop')) {
        removeActiveMenuMobile();
      }
    })
    if (!theme.isMobile()) {
      $(window).on('scroll', function (e) {
        if (!$('body').hasClass('backdrop')) return;
        removeActiveMenuMobile();
      })
    }
  }

  function codeCopy() {
    $(document).on('click', 'a[data-coupon]', function (e) {
      e.preventDefault();
      const coupon = $(this).attr('data-coupon');
      theme.Loading.show()
      $.ajax({
        url: 'https://www.coolmate.me/wishlistcoupon' + '?coupon=' + coupon + '&redirect=0',
        type: "GET",
        cache: false,
      }).done(function (res) {
        theme.Loading.hide();
        if (res.success) {
          theme.PushNotify({
            message: res.message,
            level: 'message'
          })
        }
      }).fail(() => {
        theme.Loading.hide();
        theme.PushNotify({
          message: 'Lưu mã thất bại!',
          level: 'error'
        })
      }
      ).always(() => {
        theme.Loading.hide()
      }
      )
    })
  }

  jQuery(document).ready(function ($) {
    headerSearch();
    headerScroll();
    codeCopy()
    megaMenuToggle();
    subMenuToggle();
  });
});
</script>
